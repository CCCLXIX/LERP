<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="0dp"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context="simple.prp.CCCLXIX"
    android:id="@+id/ccclxix"
    android:background="@drawable/ccclxix_background"
    android:nestedScrollingEnabled="false">

    <ImageView
        android:layout_width="30dp"
        android:layout_height="60dp"
        android:id="@+id/drop3"
        android:src="@drawable/raindrop"
        android:clickable="false"
        android:layout_marginTop="-30dp"
        android:layout_marginLeft="250dp" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="60dp"
        android:id="@+id/drop4"
        android:src="@drawable/raindrop"
        android:clickable="false"
        android:layout_marginTop="-180dp"
        android:layout_marginLeft="120dp" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="60dp"
        android:id="@+id/drop5"
        android:src="@drawable/raindrop"
        android:clickable="false"
        android:layout_marginTop="-120dp"
        android:layout_marginLeft="280dp" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="60dp"
        android:id="@+id/drop6"
        android:src="@drawable/raindrop"
        android:clickable="false"
        android:layout_marginTop="-200dp"
        android:layout_marginLeft="20dp" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="60dp"
        android:id="@+id/drop8"
        android:src="@drawable/raindrop"
        android:clickable="false"
        android:layout_marginTop="-280dp"
        android:layout_marginLeft="80dp" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="60dp"
        android:id="@+id/dropp1"
        android:src="@drawable/raindrop"
        android:clickable="false"
        android:layout_marginTop="-30dp"
        android:layout_marginLeft="200dp" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="60dp"
        android:id="@+id/dropp2"
        android:src="@drawable/raindrop"
        android:clickable="false"
        android:layout_marginTop="-180dp"
        android:layout_marginLeft="80dp" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="60dp"
        android:id="@+id/dropp3"
        android:src="@drawable/raindrop"
        android:clickable="false"
        android:layout_marginTop="-120dp"
        android:layout_marginLeft="300dp" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="60dp"
        android:id="@+id/dropp4"
        android:src="@drawable/raindrop"
        android:clickable="false"
        android:layout_marginTop="-200dp"
        android:layout_marginLeft="1dp" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="60dp"
        android:id="@+id/dropp5"
        android:src="@drawable/raindrop"
        android:clickable="false"
        android:layout_marginTop="-280dp"
        android:layout_marginLeft="60dp" />

    <ImageView
        android:layout_width="180dp"
        android:layout_height="100dp"
        android:id="@+id/imageView11"
        android:src="@drawable/jmhq_logo"
        android:layout_below="@+id/drop3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="80dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Living Environment Regents Prep."
        android:id="@+id/textView7"
        android:layout_below="@+id/imageView11"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="36dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Ver. 3.2.5"
        android:id="@+id/textView8"
        android:layout_below="@+id/textView7"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
